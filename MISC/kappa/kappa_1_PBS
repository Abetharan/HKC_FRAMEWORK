#PBS -lwalltime=24:00:00

# Load modules for any applications

module load gcc/5.4.0
module load anaconda

timeout 1420m python3 Main.py -p $HOME/kappa_1 >/dev/null
curr_cycle=$(<$HOME/kappa_1/kappa_1/restart_counter.txt)
max_cycle = $(<$HOME/kappa_1/kappa_1/RESTART/restart_counter.txt)

if [ "$curr_cycle" -lt "$max_cycle" ]; then
   qsub $JOB_NAME
fi 


